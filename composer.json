{
    "name": "drupal/recommended-project",
    "description": "Drupal 9 project",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "homepage": "https://www.drupal.org/project/drupal",
    "support": {
        "docs": "https://www.drupal.org/docs/user_guide/en/index.html",
        "chat": "https://www.drupal.org/node/314178"
    },
    "repositories": {
        "drupal": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        "asset-packagist": {
            "type": "composer",
            "url": "https://asset-packagist.org"
        },
        "domain_adv": {
            "type": "vcs",
            "url": "https://github.com/necyberteam/domain_adv.git"
        },
        "cilogin_auth": {
            "type": "vcs",
            "url": "https://github.com/necyberteam/cilogin_auth.git"
        }

    },
    "require": {
        "acquia/blt": "^12.0",
        "acquia/blt-behat": "^1.3",
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1.6",
        "drupal/auditfiles": "^3.0@beta",
        "drupal/better_exposed_filters": "^5.0",
        "drupal/better_search": "^1.6",
        "drupal/block_exclude_pages": "^2.0",
        "drupal/captcha": "^1.1",
        "drupal/change_pwd_page": "^1.0",
        "drupal/content_sync": "^3.0.0-beta1",
        "drupal/core-composer-scaffold": "^9.4",
        "drupal/core-project-message": "^9.4",
        "drupal/core-recommended": "^9.4",
        "drupal/csv_serialization": "^2.0",
        "drupal/ctools": "^3.4",
        "drupal/domain": "^1.0@beta",
        "drupal/domain_access": "^1.0@alpha",
        "drupal/domain_alias": "^1.0@alpha",
        "drupal/domain_chosen_select": "^1.0",
        "drupal/domain_config": "^1.0@alpha",
        "drupal/domain_path_redirect": "^1.3",
        "drupal/domain_site_settings": "^1.3",
        "drupal/domain_source": "^1.0@alpha",
        "drupal/domain_theme_switch": "^1.4",
        "drupal/field_permissions": "^1.1",
        "drupal/file_delete": "^1.0",
        "drupal/fixed_block_content": "^1.1",
        "drupal/flag": "^4.0@beta",
        "drupal/ga_login": "^1.0",
        "drupal/google_analytics": "^4.0",
        "drupal/honeypot": "^2.0",
        "drupal/image_captcha": "^1.1",
        "drupal/imce": "^2.2",
        "drupal/login_destination": "^2.0@beta",
        "drupal/login_disable": "^2.0",
        "drupal/mail_login": "^2.6",
        "drupal/masquerade": "^2.0@beta",
        "drupal/menu_block": "^1.6",
        "drupal/menu_item_extras": "^2.11",
        "drupal/menu_item_role_access": "^2.0",
        "drupal/menu_link_attributes": "^1.0",
        "drupal/menu_per_role": "^1.2",
        "drupal/mgv": "^1.16",
        "drupal/multiple_registration": "^3.0",
        "drupal/nocurrent_pass": "^1.0",
        "drupal/nodeaccess": "^1.0",
        "drupal/obfuscate_email": "^1.2",
        "drupal/pantheon_advanced_page_cache": "^1.2",
        "drupal/pathauto": "^1.8",
        "drupal/readonlymode": "^1.1",
        "drupal/real_aes": "^2.3",
        "drupal/realname": "^2.0@beta",
        "drupal/recaptcha": "^3.0",
        "drupal/recurring_events": "^2.0@RC",
        "drupal/redirect_after_registration": "^1.1",
        "drupal/restui": "^1.20",
        "drupal/role_delegation": "^1.1",
        "drupal/rot13": "^1.2",
        "drupal/rules": "^3.0",
        "drupal/search404": "^2.0",
        "drupal/search_exclude": "^2.0@beta",
        "drupal/select2": "^1.13",
        "drupal/simple_sitemap": "^4.0",
        "drupal/social_api": "^3.0",
        "drupal/social_auth": "^3.0",
        "drupal/social_auth_google": "^3.0",
        "drupal/swiftmailer": "^2.2",
        "drupal/taxonomy_manager": "^2.0",
        "drupal/taxonomy_term_depth": "^2.4",
        "drupal/token": "^1.7",
        "drupal/token_views_filter": "^1.4",
        "drupal/twig_tweak": "^2.6",
        "drupal/typed_data": "^1.0@alpha",
        "drupal/upgrade_rector": "^1.0@alpha",
        "drupal/user_current_paths": "^2.0",
        "drupal/views_aggregator": "^1.0-beta3",
        "drupal/views_block_filter_block": "^1.0",
        "drupal/views_bulk_operations": "^4.0",
        "drupal/views_contextual_filters_or": "^1.1",
        "drupal/views_data_export": "^1.0@RC",
        "drupal/views_field_view": "^1.0@beta",
        "drupal/views_filters_populate": "^2.0",
        "drupal/views_taxonomy_term_name_depth": "^7.0",
        "drupal/views_tree": "^2.0@alpha",
        "drupal/webform": "^6.1",
        "drupal/webform_views": "^5.0alpha",
        "drupal/yaml_editor": "^1.0",
        "drupal_fork/cilogon_auth": "dev-main",
        "drupal_fork/domain_adv": "dev-main",
        "drush/drush": "^10.2",
        "npm-asset/select2": "^4.0",
        "oomphinc/composer-installers-extender": "^2.0"
    },
    "require-dev": {
        "drupal/core-dev": "^9.4",
        "drupal/devel": "^4.1",
        "drupal/devel_kint_extras": "^1.0",
        "drupal/upgrade_status": "^3.14",
        "mglaman/drupal-check": "^1.4",
        "mikemadison13/blt-lando": "dev-main",
        "phpspec/prophecy-phpunit": "^2",
        "phpstan/phpstan-deprecation-rules": "^1.0"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "composer/installers": true,
            "cweagans/composer-patches": true,
            "drupal/core-composer-scaffold": true,
            "drupal/core-project-message": true,
            "oomphinc/composer-installers-extender": true,
            "acquia/blt": true
        }
    },
    "extra": {
      "drupal-scaffold": {
          "locations": {
              "web-root": "web/"
          },
          "file-mapping": {
              "[web-root]/README.txt": false,
              "[web-root]/sites/README.txt": false,
              "[web-root]/example.gitignore": false,
              "[web-root]/sites/example.settings.local.php": false,
              "[web-root]/sites/example.sites.php": false,
              "[web-root]/modules/README.txt": false,
              "[web-root]/profiles/README.txt": false,
              "[web-root]/themes/README.txt": false,
              "[web-root]/INSTALL.txt": false
          }
      },
        "patches-file": "composer.patches.json",
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": [
                "type:drupal-library",
                "type:bower-asset",
                "type:npm-asset"
            ],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/contrib/{$name}": ["type:drupal-drush"],
            "web/modules/custom/{$name}": ["type:drupal-custom-module"],
            "web/themes/custom/{$name}": ["type:drupal-custom-theme"]
        },
        "installer-types": ["bower-asset", "npm-asset"]
    },
    "autoload": {
        "psr-4": {
            "Example\\": "blt/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Drupal\\Tests\\PHPUnit\\": "tests/phpunit/src/"
        }
    }
}
