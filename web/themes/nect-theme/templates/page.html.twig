{#
/**
 * @file
 * Default theme implementation to display a single page.
 *
 * The doctype, html, head and body tags are not in this template. Instead they
 * can be found in the html.html.twig template in this directory.
 *
 * Available variables:
 *
 * General utility variables:
 * - base_path: The base URL path of the Drupal installation. Will usually be
 *   "/" unless you have installed Drupal in a sub-directory.
 * - is_front: A flag indicating if the current page is the front page.
 * - logged_in: A flag indicating if the user is registered and signed in.
 * - is_admin: A flag indicating if the user has permission to access
 *   administration pages.
 *
 * Site identity:
 * - front_page: The URL of the front page. Use this instead of base_path when
 *   linking to the front page. This includes the language domain or prefix.
 *
 * Page content (in order of occurrence in the default page.html.twig):
 * - messages: Status and error messages. Should be displayed prominently.
 * - node: Fully loaded node, if there is an automatically-loaded node
 *   associated with the page and the node ID is the second argument in the
 *   page's path (e.g. node/12345 and node/12345/revisions, but not
 *   comment/reply/12345).
 *
 * Regions:
 * - page.header: Items for the header region.
 * - page.primary_menu: Items for the primary menu region.
 * - page.secondary_menu: Items for the secondary menu region.
 * - page.highlighted: Items for the highlighted content region.
 * - page.help: Dynamic help text, mostly for admin pages.
 * - page.content: The main content of the current page.
 * - page.sidebar_first: Items for the first sidebar.
 * - page.sidebar_second: Items for the second sidebar.
 * - page.footer: Items for the footer region.
 * - page.breadcrumb: Items for the breadcrumb region.
 *
 * @see template_preprocess_page()
 * @see html.html.twig
 *
 * @ingroup themeable
 */
#}
<div class="wrapper">
	<a id="main-content" tabindex="-1"></a>{# link is in html.html.twig #}
	{% if page.primary_menu %}
	 {% block navbar %}
	{% if logged_in %}
	<nav class="navbar navbar-expand-lg secondary-nav navbar-light bg-light" id="secondaryNav">
	{% else %}
	<nav class="navbar navbar-expand secondary-nav navbar-light bg-light " id="secondaryNav">
	{% endif %}
	  <div class="container">
	    <div id="secondaryNavbarResponsive" style="margin-left: auto; margin-right: 0;">
		  {% if page.secondary_menu and logged_in %}
			  {{ page.secondary_menu }}
		  {% else %}
        {{ drupal_block('search_form_block') }}
        <ul class="navbar-nav ml-auto" style="margin-left: auto !important;">
          {% if domain_name == "campus-champions" %}
            <li class="nav-item">
              <a href="/user/login?current={{ path("<current>") }}" class="nav-link" data-drupal-link-system-path="user/login"><i class="fa fa-sign-in"></i> Log in/Join Portal</a>
            </li>
            <li class="nav-item">
              <a href="/form/join-campus-champions" class="nav-link">Join CC</a>
            </li>
          {% else %}
            {% if domain_name == "coco" %}
            <li class="nav-item">
              <a href="/user/register/representative" class="nav-link">Join</a>
            </li>
            <li class="nav-item">
              <a href="/user/login?current=/" class="nav-link" data-drupal-link-system-path="user/login">Log in</a>
            </li>
            {% else %}
            <li class="nav-item">
              <a href="/user/register" class="nav-link" data-drupal-link-system-path="user/register">Join</a>
            </li>
            <li class="nav-item">
              <a href="/user/login?current={{ path("<current>") }}" class="nav-link" data-drupal-link-system-path="user/login">Log in</a>
            </li>
            {% endif %}
          {% endif %}
        </ul>
		  {% endif %}
      </div>
	  </div>
	</nav>
	 <nav class="navbar fixed-top navbar-expand-lg navbar-light bg-light shadow-sm" id="mainNav">
		  <div class="container">
			<a class="navbar-brand" href="/">
        {% if domain_name == "northeast-cyberteam" %}
        <img src="/themes/nect-theme/img/logo.png" class="logo" alt="Northeast Cyberteam">
        {% elseif domain_name == "great-plains-cyberteam" %}
        <img src="/themes/nect-theme/img/GP.cyberteam_logo.jpg" class="logo" alt="Great Plains Cyberteam">
        {% elseif domain_name == "rmacc-cyberteam" %}
        <img src="/themes/nect-theme/img/rmacc.ctlogo.png" class="logo" alt="RMACC Cyberteam">
        {% elseif domain_name == "careers-cyberteam" %}
        <img src="/themes/nect-theme/img/careerslogo.flat.png" class="logo" alt="CAREERS Cyberteam">
        {% elseif domain_name == "kentucky-cyberteam" %}
        <img src="/themes/nect-theme/img/kyctlogo.png" class="logo" alt="Kentucky Cyberteam">
        {% elseif domain_name == "sweeter-cyberteam" %}
        <img src="/themes/nect-theme/img/sweeter.logo.png" class="logo" alt="SWEETER Cyberteam">
        {% elseif domain_name == "mines-cyberteam" %}
        <img src="/themes/nect-theme/img/mines.logo.png" class="logo" alt="MINES Cyberteam">
        {% elseif domain_name == "trecis-cyberteam" %}
        <img src="/themes/nect-theme/img/trecis.logo.png" class="logo" alt="TRECIS Cyberteam">
        {% elseif domain_name == "campus-champions" %}
        <img src="/themes/nect-theme/img/campuschampions.png" class="logo" alt="Campus Champions">
        {% elseif domain_name == "connect-ci" %}
        <img src="/themes/nect-theme/img/ConnectCI-Logo.jpg" class="logo" alt="Connect CI">
        {% elseif domain_name == "coco" %}
        <img src="/themes/nect-theme/img/coco-logo.png" class="logo" alt="CoCo">
        {% elseif domain_name == "usrse" %}
        <img src="/themes/nect-theme/img/usrse-logo.png" class="logo" alt="USRSE">
        {% endif %}
      </a>
      {% if domain_name != "connect-ci" and domain_name != "coco" and domain_name != "usrse" %}
			<button class="navbar-toggler navbar-toggler-right" type="button" data-toggle="collapse" data-target="#navbarResponsive" aria-controls="navbarResponsive" aria-expanded="false" aria-label="Toggle navigation">
			  <i class="fa fa-bars"></i>
			</button>
			<div class="collapse navbar-collapse" id="navbarResponsive">
			{% if page.primary_menu %}
				  {{ page.primary_menu }}
			{% endif %}
			</div>
      {% endif %}
		</div>
	 </nav>
	 {% endblock %}
	{% endif %}
	{% if is_front and domain_name != "campus-champions" %}
    {% if domain_name == "northeast-cyberteam" %}
    <header class="masthead" style="background-image: url('/themes/nect-theme/img/background.jpg')">
    {% elseif domain_name == "great-plains-cyberteam" %}
    <header class="masthead" style="background-image: url('/themes/nect-theme/img/greatplainsct.background.jpg')">
    {% elseif domain_name == "rmacc-cyberteam" %}
    <header class="masthead" style="background-image: url('/themes/nect-theme/img/rmacc.background.jpg')">
    {% elseif domain_name == "careers-cyberteam" %}
    <header class="masthead" style="background-image: url('/themes/nect-theme/img/crct.background.jpg')">
    {% elseif domain_name == "kentucky-cyberteam" %}
    <header class="masthead" style="background-image: url('/themes/nect-theme/img/kyct.background.jpg')">
    {% elseif domain_name == "sweeter-cyberteam" %}
    <header class="masthead" style="background-image: url('/themes/nect-theme/img/sweeter.background.jpg')">
    {% elseif domain_name == "mines-cyberteam" %}
    <header class="masthead" style="background-image: url('/themes/nect-theme/img/mines.background.jpg')">
    {% elseif domain_name == "trecis-cyberteam" %}
    <header class="masthead" style="background-image: url('/themes/nect-theme/img/trecis.background.jpg')">
    {% elseif domain_name == "connect-ci" %}
    <header class="masthead" style="background-image: url('/themes/nect-theme/img/background.jpg')">
    {% elseif domain_name == "coco" %}
    <header class="masthead" style="background-color:#781346">
    {% else %}
    <header class="masthead" style="background-image: url('/themes/nect-theme/img/background.jpg')">
    {% endif %}
      <div class="container">
        <div class="row">
          <div class="col-12 mx-auto">
            <div class="site-heading">
              <h2 class="mx-auto my-0">
              {% if domain_name == "northeast-cyberteam" %}
                {{ nect_front_text | raw }}
              {% elseif domain_name == "great-plains-cyberteam" %}
                {{ gpct_front_text | raw }}
              {% elseif domain_name == "rmacc-cyberteam" %}
                {{ rmacc_front_text | raw }}
              {% elseif domain_name == "careers-cyberteam" %}
                {{ careers_front_text | raw }}
              {% elseif domain_name == "kentucky-cyberteam" %}
                {{ kentucky_front_text | raw }}
              {% elseif domain_name == "sweeter-cyberteam" %}
                {{ sweeter_front_text | raw }}
              {% elseif domain_name == "mines-cyberteam" %}
                {{ mines_front_text | raw }}
              {% elseif domain_name == "trecis-cyberteam" %}
                {{ trecis_front_text | raw }}
              {% elseif domain_name == "connect-ci" %}
                {{ connectci_front_text | raw }}
              {% elseif domain_name == "coco" %}
                {{ coco_front_text | raw }}
              {% elseif domain_name == "usrse" %}
                {{ usrse_front_text | raw }}
              {% endif %}
              </h2>
              <ul class="d-none fa-ul mt-5 mx-auto">
                <li class="d-inline mx-3"><a href="/about" class="btn btn-sm-block btn-outline-light btn-pill px-5">About The Team</a></li>
                <li class="d-inline mx-3"><a href="/blog" class="btn btn-sm-block btn-outline-light btn-pill px-5">Blog</a></li>
              </ul>
            </div>
          </div>
        </div>
		  </div>
		</header>
	{% else %}
		{% if page.banner_image %}
			{% set escdiv = 'div' %}
			<header class="masthead" style="background-image: url('{% autoescape 'div' %}{% spaceless %}{{- page.banner_image -}}{% endspaceless %}{% endautoescape %}')">
			  <div class="overlay"></div>
			  <div class="container">
				<div class="row">
				  <div class="col-lg-8 col-md-10 mx-auto">
					<div class="site-heading">

					</div>
				  </div>
				</div>
			  </div>
			</header>
		{% endif %}
	{% endif %}

	{{ page.header }}

	{% if is_front %}<div class="w-100 py-3 bg-light">{% endif %}

        {# grey background #}
        {% if (view_id == 'organization') and (display_id == 'organizations') %}
        <div class="w-100 py-3 bg-light">
        {% endif %}

	<div class="container{% if is_front %} main-container mt-3{{ domain_name == "campus-champions" ? ''}}{% else %} mt-4{% endif %}">
	  {% if domain_name == "campus-champions" and is_front %}
      {% if page.campus_champions_front_header %}
        <div class="row">
          {{ page.campus_champions_front_header }}
        </div>
      {% endif %}
      {% if page.campus_champions_front_quick_links_join or page.campus_champions_front_quick_links_top_tags or page.campus_champions_front_quick_links_least_tags or page.campus_champions_front_quick_links_help %}
        <div class="row mx-auto border-top py-3 text-center">
          <div class="cc-heading col-12 my-3"><h3>Quick Links</h3></div>
          {{ page.campus_champions_front_quick_links_join }}
          <div class="col-md-4 mb-3">
          <div class="card h-100">
            <div class="card-body text-center">
              <svg aria-hidden="true" focusable="false" data-prefix="fas" data-icon="tags" class="svg-inline--fa fa-tags w-25 p-3 img-fluid" role="img" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 640 512"><path fill="currentColor" d="M497.941 225.941L286.059 14.059A48 48 0 0 0 252.118 0H48C21.49 0 0 21.49 0 48v204.118a48 48 0 0 0 14.059 33.941l211.882 211.882c18.744 18.745 49.136 18.746 67.882 0l204.118-204.118c18.745-18.745 18.745-49.137 0-67.882zM112 160c-26.51 0-48-21.49-48-48s21.49-48 48-48 48 21.49 48 48-21.49 48-48 48zm513.941 133.823L421.823 497.941c-18.745 18.745-49.137 18.745-67.882 0l-.36-.36L527.64 323.522c16.999-16.999 26.36-39.6 26.36-63.64s-9.362-46.641-26.36-63.64L331.397 0h48.721a48 48 0 0 1 33.941 14.059l211.882 211.882c18.745 18.745 18.745 49.137 0 67.882z"></path></svg>
              <h5 class="card-title">Tags</h5>
              <ul class="nav nav-tabs small d-flex justify-content-center" id="tags-tabs" role="tablist">
                <li class="nav-item" role="presentation">
                  <a class="nav-link px-2 py-1 active" id="top-tag-tab" data-toggle="tab" href="#top-tags" role="tab" aria-controls="top-tags" aria-selected="true">Top Used</a>
                </li>
                <li class="nav-item" role="presentation">
                  <a class="nav-link px-2 py-1" id="least-tag-tab" data-toggle="tab" href="#least-tags" role="tab" aria-controls="least-tags" aria-selected="false">Least Used</a>
                </li>
              </ul>
              <div class="tab-content m-2" id="tags-tabs-content">
                <div class="tab-pane fade active show" id="top-tags" role="tabpanel" aria-labelledby="top-tag-tab">
                  {{ page.campus_champions_front_quick_links_top_tags }}
                </div>
                <div class="tab-pane fade" id="least-tags" role="tabpanel" aria-labelledby="least-tag-tab">
                  {{ page.campus_champions_front_quick_links_least_tags }}
                </div>
                <a href="/tags" class="mt-3 border border-5 p-2 font-weight-bold">All Tags</a>
              </div>
            </div>
          </div>
        </div>
          {{ page.campus_champions_front_quick_links_help }}
        </div>
      {% endif %}
      {% if page.campus_champions_front_community_events %}
        <div class="row mx-auto border-top py-3">
          <div class="cc-heading col-12 my-3 text-center"><h3>Community Events</h3></div>
        </div>
				{{ page.campus_champions_front_community_events }}
      {% endif %}
      {% if page.campus_champions_front_blog %}
        <div class="row mx-auto border-top py-3">
          <div class="cc-heading col-12 my-3 text-center"><h3>Blog</h3></div>
          {{ page.campus_champions_front_blog }}
        </div>
      {% endif %}
      {% if page.campus_champions_front_jobs %}
        <div class="row mx-auto border-top py-3">
          {{ page.campus_champions_front_jobs }}
        </div>
      {% endif %}
    {% else %}
      <div class="row">
  		{% if page.primary_sidebar or page.secondary_sidebar %}
  			<div class="col-lg-8 col-12 mx-auto">
          {{ page.page_top }}
          {{ page.title_area }}
          {{ page.view_type_buttons }}
          {{ page.drupal_tabs }}
          {{ page.exposed_filters }}
          {{ page.tags_page_resources }}
          {{ page.tags_page_projects }}
          {{ page.tags_page_students }}
          {{ page.tags_page_mentors }}
          {{ page.tags_page_researchers }}
          {{ page.tags_page_blog }}
          {{ page.tags_page_askci }}
          {{ page.people_page_card }}
          {{ page.people_page_list }}
          {{ page.projects_page }}
          {{ page.resources_page }}
          {{ page.blog_page }}
          {{ page.help_desk }}
          {{ page.user_profile }}
          {{ page.content }}
          {{ page.below_main_content }}
  			</div>
  			<div class="col-lg-4 col-12 mx-auto">
  				{{ page.primary_sidebar }}
  				{{ page.secondary_sidebar }}
  			</div>
  		{% else %}
  			<div class="col-12 mx-auto">
          {{ page.page_top }}
          {{ page.title_area }}
          {{ page.view_type_buttons }}
          {{ page.drupal_tabs }}
          {{ page.exposed_filters }}
	  {{ page.tags_page_mentors }}
	  {{ page.tags_page_askci }}
          {{ page.tags_page_students }}
          {{ page.tags_page_researchers }}
	  {{ page.tags_page_resources }}
          {{ page.tags_page_projects }}
          {{ page.tags_page_blog }}
          {{ page.people_page_card }}
          {{ page.people_page_list }}
          {{ page.projects_page }}
          {{ page.resources_page }}
          {{ page.blog_page }}
          {{ page.help_desk }}
          {{ page.user_profile }}
          {{ page.content }}
          {{ page.below_main_content }}
  			</div>
  		{% endif %}
  	  </div>
    {% endif %}
	</div>
	{% if is_front %}</div>{% endif %}

        {# end grey background #}
        {% if (view_id == 'organization') and (display_id == 'organizations') %}
        <div class="w-100 py-3 bg-light">
        {% endif %}
</div>{# /.wrapper #}

<footer class="footer">
  {% if page.first_footer or page.second_footer or page.third_footer %}
  <div class="container-fluid footer-main pt-3">
	<div class="row mx-auto">
	  {% if page.first_footer %}
	  {% if page.second_footer and page.third_footer %}
	  <div class="col-md-4 mx-auto">
	  {% elseif (page.second_footer and not page.third_footer) or (page.third_footer and not page.second_footer) %}
	  <div class="col-md-6 mx-auto">
	  {% else %}
	  <div class="col-12 mx-auto">
	  {% endif %}
		{{ page.first_footer }}
	  </div>
	  {% endif %}
	  {% if page.second_footer %}
	  {% if page.first_footer and page.third_footer %}
	  <div class="col-md-4 mx-auto">
	  {% elseif (page.first_footer and not page.third_footer) or (page.third_footer and not page.first_footer) %}
	  <div class="col-md-6 mx-auto">
	  {% else %}
	  <div class="col-12 mx-auto">
	  {% endif %}
		{{ page.second_footer }}
	  </div>
	  {% endif %}
	  {% if page.third_footer %}
	  {% if page.first_footer and page.second_footer %}
	  <div class="col-md-4 mx-auto">
	  {% elseif (page.first_footer and not page.second_footer) or (page.second_footer and not page.first_footer) %}
	  <div class="col-md-6 mx-auto">
	  {% else %}
	  <div class="col-12 mx-auto">
	  {% endif %}
		{{ page.third_footer }}
	  </div>
	  {% endif %}
	</div>
  </div>
  {% endif %}
  <div class="container-fluid footer-sub">
	<div class="row">
	  <div class="col-lg-12 col-md-10 mx-auto">
                <p class="copyright">Funded in part by the <a href="https://nsf.gov" target="_blank" style="color: #fff;">National Science Foundation</a></p>
		<p class="copyright">Copyright &copy; {{ 'now'|date('Y') }}, {{ site_name }} &bull; All Rights Reserved &bull; <a href="/regions" style="color: white;">Connect.CI</a> &bull; <a href="/contact/{{domain_name|replace({'-':'_'})}}" style="color: white;">Contact Us</a></p>
	  </div>
	</div>
  </div>
</footer>
<script>
  window.onUsersnapCXLoad = function(api) {
    api.init();
  }
  var script = document.createElement('script');
  script.defer = 1;
  script.src = 'https://widget.usersnap.com/global/load/7c797226-874a-4785-aac2-c91bdc27b084?onload=onUsersnapCXLoad';
  document.getElementsByTagName('head')[0].appendChild(script);
</script>
